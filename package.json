{
  "name": "flyerscord-helper",
  "version": "2.0.0",
  "description": "Flyers' Discord Helper Bot",
  "main": "dist/bot.js",
  "scripts": {
    "build": "npx tsc",
    "start:prod": "node /usr/src/app/src/bot.js",
    "start:dev": "(npx tsc -w &) && npx nodemon",
    "lint": "npx eslint --ext .js,.ts .",
    "lintfix": "npx eslint . --fix",
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "update": "npx npm-check -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strenkml/discord-typescript-template.git"
  },
  "author": "Matt Strenk",
  "license": "GPLV3",
  "bugs": {
    "url": "https://github.com/strenkml/discord-typescript-template/issues"
  },
  "homepage": "https://github.com/strenkml/discord-typescript-template#readme",
  "dependencies": {
    "axios": "^1.7.7",
    "child_process": "^1.0.2",
    "discord.js": "^14.16.2",
    "enmap": "^6.0.2",
    "imgur": "^2.4.2",
    "lodash": "^4.17.21",
    "mee6-levels-api": "^1.3.1",
    "moment": "^2.30.1",
    "stumper": "^1.0.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.5",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.5",
    "npm-check": "^6.0.1",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
