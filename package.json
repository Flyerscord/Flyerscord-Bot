{
  "name": "flyerscord-helper",
  "version": "2.0.2",
  "description": "Flyers' Discord Helper Bot",
  "main": "dist/bot.js",
  "scripts": {
    "copy-assets": "npx copyfiles -u 1 src/**/*.png dist/",
    "build": "npx tsc && npm run copy-assets",
    "build:no-assets": "npx tsc",
    "start:prod": "node /usr/src/app/src/bot.js",
    "start:dev": "npm run build:no-assets && node dist/bot.js",
    "lint": "npx eslint .",
    "lintfix": "npx eslint . --fix",
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "update": "npx npm-check -u",
    "depcheck": "npx npm-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strenkml/NHL-Helper-Bot.git"
  },
  "author": "Matt Strenk",
  "license": "GPLV3",
  "bugs": {
    "url": "https://github.com/strenkml/NHL-Helper-Bot/issues"
  },
  "homepage": "https://github.com/strenkml/NHL-Helper-Bot#readme",
  "dependencies": {
    "@atproto/api": "^0.13.29",
    "@discordjs/builders": "^1.9.0",
    "axios": "^1.7.7",
    "canvas": "^2.11.2",
    "discord.js": "^14.16.3",
    "enmap": "^5.9.10",
    "express": "^4.21.1",
    "imagekit": "^5.2.0",
    "imgur": "^2.4.2",
    "jsdom": "^25.0.1",
    "mee6-levels-api": "^1.3.1",
    "nhl-api-wrapper-ts": "^1.0.0",
    "node-schedule": "^2.1.1",
    "request": "^2.88.2",
    "sharp": "^0.33.5",
    "stumper": "^1.0.5"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.8.2",
    "@types/node-schedule": "^2.1.7",
    "@types/request": "^2.48.12",
    "@typescript-eslint/eslint-plugin": "^8.12.1",
    "@typescript-eslint/parser": "^8.12.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "npm-check": "^6.0.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  }
}
