{
  "name": "flyerscord-helper",
  "version": "2.1.7",
  "description": "Flyers' Discord Helper Bot",
  "main": "src/bot.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json --noEmit",
    "build:test": "tsc -p tsconfig.test.json --noEmit",
    "start": "tsx src/bot.ts",
    "start:dev": "pnpm run build && tsx src/bot.ts",
    "start:watch": "tsx watch src/bot.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(ts|json)\"",
    "update": "npm-check -u",
    "depcheck": "npm-check",
    "generate-config": "tsx src/createDefaultConfig.ts",
    "prepare": "husky",
    "test": "jest --no-coverage",
    "test:coverage": "jest --coverage",
    "circular-deps": "madge --circular --extensions ts src",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flyerscord/Flyerscord-Bot.git"
  },
  "author": "Matt Strenk",
  "license": "GPLV3",
  "bugs": {
    "url": "https://github.com/Flyerscord/Flyerscord-Bot/issues"
  },
  "homepage": "https://github.com/Flyerscord/Flyerscord-Bot#readme",
  "dependencies": {
    "@atproto/api": "^0.15.19",
    "@discordjs/builders": "^1.11.2",
    "@neondatabase/serverless": "^0.10.4",
    "axios": "^1.10.0",
    "canvas": "^2.11.2",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "enmap": "^5.9.10",
    "express": "^4.21.1",
    "imagekit": "^5.2.0",
    "imgur": "^2.5.0",
    "jsdom": "^25.0.1",
    "nhl-api-wrapper-ts": "^1.0.0",
    "node-schedule": "^2.1.1",
    "request": "^2.88.2",
    "sharp": "^0.33.5",
    "stumper": "^1.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.19",
    "@types/node": "^24.5.2",
    "@types/node-schedule": "^2.1.7",
    "@types/request": "^2.48.12",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "copyfiles": "^2.4.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "madge": "^8.0.0",
    "npm-check": "^6.0.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.6",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "madge": {
    "detectiveOptions": {
      "ts": {
        "skipAsyncImports": true,
        "skipTypeImports": true
      }
    }
  }
}
