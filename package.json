{
  "name": "flyerscord-helper",
  "version": "2.0.0",
  "description": "Flyer's Discord Helper Bot",
  "main": "dist/bot.js",
  "scripts": {
    "build": "npx tsc",
    "start:prod": "node /usr/src/app/src/bot.js",
    "start:dev": "(npx tsc -w &) && npx nodemon",
    "lint": "npx eslint --ext .js,.ts .",
    "lintfix": "npx eslint . --fix",
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "update": "npx npm-check -u",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strenkml/discord-typescript-template.git"
  },
  "author": "Matt Strenk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strenkml/discord-typescript-template/issues"
  },
  "homepage": "https://github.com/strenkml/discord-typescript-template#readme",
  "dependencies": {
    "axios": "^1.6.7",
    "child_process": "^1.0.2",
    "discord.js": "^14.14.1",
    "enmap": "^5.9.8",
    "lodash": "^4.17.21",
    "mee6-levels-api": "^1.3.1",
    "moment": "^2.30.1",
    "stumper": "^1.0.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "npm-check": "^6.0.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
