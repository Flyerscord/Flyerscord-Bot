{
  "name": "flyerscord-helper",
  "version": "2.0.0",
  "description": "Flyers' Discord Helper Bot",
  "main": "dist/bot.js",
  "scripts": {
    "build": "npx tsc",
    "start:prod": "node /usr/src/app/src/bot.js",
    "start:dev": "(npx tsc -w &) && npx nodemon",
    "lint": "npx eslint .",
    "lintfix": "npx eslint . --fix",
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "update": "npx npm-check -u",
    "depcheck": "npx npm-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strenkml/discord-typescript-template.git"
  },
  "author": "Matt Strenk",
  "license": "GPLV3",
  "bugs": {
    "url": "https://github.com/strenkml/discord-typescript-template/issues"
  },
  "homepage": "https://github.com/strenkml/discord-typescript-template#readme",
  "dependencies": {
    "axios": "^1.7.7",
    "canvas": "^2.11.2",
    "child_process": "^1.0.2",
    "discord.js": "^14.16.3",
    "enmap": "^6.0.3",
    "express": "^4.21.1",
    "imgur": "^2.4.2",
    "jsdom": "^25.0.1",
    "lodash": "^4.17.21",
    "mee6-levels-api": "^1.3.1",
    "moment": "^2.30.1",
    "nhl-api-wrapper-ts": "^1.0.0",
    "node-schedule": "^2.1.1",
    "stumper": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.8.1",
    "@types/node-schedule": "^2.1.7",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "npm-check": "^6.0.1",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  }
}
